    <style>

        .dropdown-toggle {
            color: black;
            font-size: 10px;
        }

        .pos {
            position: relative;
            top: 20px;
            margin: 15px;
            font-size: 15px;
        }

        .posB {
            position: relative;
            top: 5px;
            margin: 15px;
            font-size: 15px;
        }


        .dropdown-menu {
        }

            .dropdown-menu.columns-2 {
                min-width: 400px;
            }

            .dropdown-menu.columns-3 {
                min-width: 600px;
            }

            .dropdown-menu li a {
                padding: 5px 15px;
                font-weight: 300;
            }

        .multi-column-dropdown {
            list-style: none;
            margin: 0px;
            padding: 0px;
            color: rgba(0,0,0,0.6);
            text-shadow: 2px 8px 6px rgba(0,0,0,0.2), 0px -5px 35px rgba(255,255,255,0.3);
        }

            .multi-column-dropdown li a {
                display: block;
                clear: both;
                line-height: 1.428571429;
                color: #333;
                white-space: normal;
            }

                .multi-column-dropdown li a:hover {
                    text-decoration: none;
                    color: #262626;
                    background-color: #999;
                }

        @@media (max-width: 767px) {
            .dropdown-menu.multi-column {
                min-width: 240px !important;
                overflow-x: hidden;
            }
        }

        .chip {
            display: inline-block;
            padding: 0 3px;
            height: 30px;
            font-size: 16px;
            line-height: 30px;
            border-radius: 25px;
            background-color: #f1f1f1;
        }

            .chip img {
                float: left;
                margin: 0 10px 0 0px;
                height: 30px;
                width: 30px;
                border-radius: 50%;
            }

        .label {
            color: white;
            padding: 8px;
        }

        .success {
            background-color: #04AA6D;
            line-height: 30px;
            border-radius: 25px;
        }

        .warning {
            background-color: #ff9800;
            line-height: 30px;
            border-radius: 35px;
        }
        /* Orange */
        /* Green */
